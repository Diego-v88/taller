<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="entities.Notification" table="notification" schema="public" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="senddate" type="date">
            <column name="senddate" length="13" not-null="true" />
        </property>
        <property name="nextTurns" type="date">
            <column name="nextturns" length="13" not-null="true" />
        </property>
        <property name="fechaBaja" type="date">
            <column name="fechabaja" length="13" not-null="true" />
        </property>
        <!--<many-to-one name="guard" class="entities.Guard" fetch="select">
            <column name="fk_guard" not-null="true" />
        </many-to-one>-->
    </class>
</hibernate-mapping>
